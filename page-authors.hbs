{{!< default}}
{{! The tag above means: insert everything in this file into the body of the default.hbs template }}

{{#get "authors" limit="all"}}
  {{#foreach authors}}
    <div class="author-card">
      <img src="{{profile_image}}" alt="{{name}}" />
      <h3><a href="{{url}}">{{name}}</a></h3>
      <p>{{bio}}</p>
    </div>
  {{/foreach}}
{{/get}}
